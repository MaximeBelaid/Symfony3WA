{%  extends "TroiswaBackBundle::layout-bo.html.twig" %}

{%  block titre %}Feedback{{ parent() }}{% endblock %}

{% block titreContent %}Feedback{%  endblock %}

{% form_theme formFeedback "TroiswaBackBundle:Form:fields.html.twig" %}

{% block content %}
    <div class="row">
        <div class="col-md-12">

            {% for flashMessage in app.session.flashbag.get("success_feedback") %}

                <div class="alert alert-success alert-dismissable">
                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
                    <h4>	<i class="icon fa fa-check"></i> Alert!</h4>
                    {{ flashMessage }}
                </div>
            {% endfor %}
            {{ form(formFeedback,{attr:{novalidate:"novalidate"}}) }}
        </div>
    </div>
{% endblock %}
{# toto #}